    form.inline(action="al_proofs/launch" method="POST")
      button.inline(type="submit" name="mode" value="tautologies_1") Stufe 1
      button.inline(type="submit" name="mode" value="tautologies_2") Stufe 2
      button.inline(type="submit" name="mode" value="tautologies_3") Stufe 3
  div
    h3.indent-2.inline Prämissen
    form.inline(action="al_proofs/launch" method="POST")
      button.inline(type="submit" name="mode" value="premises_1") Stufe 1
      button.inline(type="submit" name="mode" value="premises_2") Stufe 2
      button.inline(type="submit" name="mode" value="premises_3") Stufe 3
  div
    h3.indent-2.inline
    form.inline(action="al_proofs/launch" method="POST")
      button.inline(type="submit" name="mode" value="sandbox") Sandbox
